{
  "name": "abimongo_utils",
  "version": "0.4.1",
  "main": "./dist/cjs/index.js",
  "type": "module",
  "typings": "./dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "browser": {
    "async_hooks": false
  },
  "repository": {
    "type": "git"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "jest --detectOpenHandles --coverage",
    "test:watch": "tsc --noEmit && jest --watch",
    "docs": "typedoc --out docs src/index.ts",
    "clean": "rimraf dist",
    "patch:version": "npm version patch",
    "minor:version": "npm version minor",
    "major:version": "npm version major",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "sideEffects": false,
  "keywords": [
    "abimongo_utils",
    "logger",
    "Advance logging"
  ],
  "author": "Emmanuel Nodolomwanyi",
  "maintainers": [
    {
      "name": "Emmanuel Nodolomwanyi",
      "email": "padave805@gmail.com"
    }
  ],
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.18",
    "@types/webpack": "^5.28.5",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.4",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-remove-debug": "^0.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1",
    "axios": "^1.9.0",
    "chalk": "^4.1.2",
    "streamroller": "^3.1.5"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
